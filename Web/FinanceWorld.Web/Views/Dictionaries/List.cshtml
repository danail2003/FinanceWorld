@model FinanceWorld.Web.ViewModels.Dictionaries.DictionaryListViewModel
@{ 
    this.ViewData["Title"] = "Financial terms";
}

<h1 class="text-center">@this.ViewData["Title"]</h1>
<hr />
<div>
    @foreach (var term in Model.Terms)
    {
        <a asp-action="ById" asp-route-id="@term.Id">@term.Name</a>
    }
</div>
